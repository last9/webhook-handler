# Server Configuration
PORT=3000

# Jira Configuration
# Your Jira domain (e.g., your-domain.atlassian.net)
JIRA_DOMAIN=your-domain.atlassian.net
# Email associated with your Jira account
JIRA_EMAIL=your-email@example.com
# API token from https://id.atlassian.com/manage/api-tokens
JIRA_API_TOKEN=your-api-token
# Project key where issues will be created
JIRA_PROJECT_KEY=PROJ
# Optional: Assignee ID (can be found in Jira user settings)
JIRA_ASSIGNEE_ID=optional-assignee-id 